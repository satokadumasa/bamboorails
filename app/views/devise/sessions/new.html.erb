        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
        <h4>ログイン</h4>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "form-inline justify-content-end md-12"}) do |f| %>
          <div class="form-group col-12 col-md-4">
            <%= f.label :email, class: 'control-label form-control-static login_label' %>
            <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'form-control', placeholder: 'mail-address' %>
          </div>
          <div class="form-group col-12 col-md-4">
            <%= f.label :password, class: 'control-label form-control-static login_label' %>
            <%= f.password_field :password, autocomplete: "password", class: 'form-control', placeholder: 'password'  %>
          </div>
          <div class="form-group col-12 col-md-4">
            <%= f.submit "Log in", class: 'btn btn-primary login_label' %>
          </div>
          <% if devise_mapping.rememberable? %>
            <div class="form-group col-12 col-md-12">
              <%= f.label :remember_me, class: 'control-label form-control-static login_label' %>
              <%= f.check_box :remember_me, class: 'form-control' %>
            </div>
          <% end %>
        <% end %>
        <div class="col-lg-12 col-md-12 col-sm-12 col-12">
          <%= render "devise/shared/links" %>
        </div>


